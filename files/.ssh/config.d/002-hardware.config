# Home Macs
Host imac
    HostName iMac.local
    User stephen

Host mbp
    HostName MacBook-Pro.local
    User stephen

# NAS servers
Host mycloud
    HostName mycloudmirror.local
    User sshd
    HostkeyAlgorithms ssh-dss
    LocalCommand echo -e "\e[1;5;32mDo not forget to run 'source /mnt/HD/HD_a2/scripts/profile'\e[0;m"
    PermitLocalCommand yes

Host mypassport
    HostName mypassport.local
    User root

Host cluster
	HostName master.local
	User pirate

Host node-1
	Hostname 10.0.0.2
	ForwardAgent yes
	User pirate
	ProxyCommand ssh -A pirate@master.local -W %h:%p

Host node-2
	Hostname 10.0.0.3
	ForwardAgent yes
	User pirate
	ProxyCommand ssh -A pirate@master.local -W %h:%p

Host node-3
	Hostname 10.0.0.4
	ForwardAgent yes
	User pirate
	ProxyCommand ssh -A pirate@master.local -W %h:%p
